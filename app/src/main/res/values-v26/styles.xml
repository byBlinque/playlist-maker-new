<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SettingsAcMenuItemWrapper">
        <item name="android:layout_height">@dimen/settings_menu_item_height_size</item>
        <item name="android:paddingHorizontal">@dimen/settings_menu_item_padding_horizontal</item>
    </style>

    <style name="MainAcButton">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="android:textSize">@dimen/main_button_text_size</item>
        <item name="cornerRadius">@dimen/main_button_corner_radius</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">@dimen/main_button_icon_padding</item>
        <item name="android:iconTint">@color/text_color</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textStyle">bold</item>
        <item name="android:letterSpacing">0</item>
    </style>
</resources>